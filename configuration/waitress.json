{
    "ports": {
        "server": 2005,
        "bases": {
            "public": 3000
        },
        "steps": {
            "latest": 5,
            "website": 10,
            "github": 3000
        }
    },
    "directories": {
        "waitress": "/var/www/waitress",
        "websites": "/var/www/waitress-websites/"
    },
    "templates": {
        "nginx": {
            "public": "/var/www/waitress/back/templates/nginx/websites/public",
            "latest": "/var/www/waitress/back/templates/nginx/websites/latest"
        }
    },
    "files": {
        "github": {
            "json": "configuration/github.json"
        },
        "ports": {
            "json": "configuration/ports.json"
        },
        "user": {
            "json": "configuration/user.json"
        },
        "logs": {
            "json": "logs/server.log"
        }
    },
    "characters": {
        "separators": {
            "website": {
                "replaced": "+",
                "replacer": " "
            }
        }
    }
}